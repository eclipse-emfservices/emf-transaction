# Debugging options for the org.eclipse.emf.transaction.ui plug-in

# Turn on general debugging for the org.eclipse.emf.transaction.ui plug-in
org.eclipse.emf.transaction.ui/debug=true


# Trace when exceptions are caught
org.eclipse.emf.transaction.ui/debug/exceptions/catching=false

# Trace when exceptions are thrown
org.eclipse.emf.transaction.ui/debug/exceptions/throwing=false


# Trace when methods are entered
org.eclipse.emf.transaction.ui/debug/methods/entering=false

# Trace when methods are exited
org.eclipse.emf.transaction.ui/debug/methods/exiting=false
